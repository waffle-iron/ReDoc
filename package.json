{
  "name": "sgr",
  "version": "0.0.1",
  "description": "This project aims to build automaticaly a showcase based component like Reactjs, Angular 2 or Polymer",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "ava",
    "lint": "eslint ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-republic/SGR.git"
  },
  "keywords": [
    "Documentation",
    "Showcase",
    "Generator",
    "React",
    "Angular 2",
    "VueJS",
    "Polymer"
  ],
  "author": "Mathieu Breton<mathieu.breton@js-republic.com>",
  "contributors": [
    "Pierrick Turelier<pierrick.turelier@js-republic.com>",
    "Vincent Cordobes<vincent.cordobes@js-republic.com>",
    "Mathieu Breton<mathieu.breton@js-republic.com>"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/js-republic/SGR/issues"
  },
  "homepage": "https://github.com/js-republic/SGR#readme",
  "devDependencies": {
    "ava": "0.17.0",
    "babel-eslint": "7.1.1",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.10.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.7.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "powerAssert": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "node": true
    },
    "extends": "airbnb"
  }
}
